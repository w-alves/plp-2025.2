{
  var x = 10;
  if x in (0..10) then write("x in (0..10): true") else write("x in (0..10): false");
  if x in (0..10] then write("x in (0..10]: true") else write("x in (0..10]: false");
  if x in [0..10) then write("x in [0..10): true") else write("x in [0..10): false");
  if x in [0..10] then write("x in [0..10]: true") else write("x in [0..10]: false");
  skip
}