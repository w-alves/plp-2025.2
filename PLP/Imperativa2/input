{
   var count = 0,
   var message = "Falha",
   func side_effect_func() : int {
      count := count + 1;
      return 5
   };
   if (side_effect_func() in [side_effect_func()..10)) then (
      message := "Sucesso";
      write(message);
      write(count)
   ) else (
      message := "Falha";
      write(message);
      write(count)
   )
}